{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "babel-module": {
    "extensions": [".js", ".ts"]
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off"
  }
}
